<!doctype html>
<html>
 <head>
  <meta charset="UTF-8">
  
  <title>Document</title>

  <style>
	#a1,#a2,#a3,#a4,#a5{
		width:100px;
		height:100px;
	}
	#a1{
		display:inline;
	}

	#a2{
		display:block;
	}
	#a3{
		display:inline-block;
	}
	#a4{
		display:none;
	}

	#a5{
		display:table;
	}
  </style>
 </head>
 <body>
	<a id ="a1">a1</a>
	<a id ="a2">a2</a>
	<a id ="a3">a3</a>
	<a id ="a4">a4</a>
	<a id ="a5">a5</a>
 </body>
</html>
